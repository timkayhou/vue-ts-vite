{
  "name": "vue-vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "eslint": "eslint --cache --cache-location \"node_modules\\.cache\\.eslintcache\" --max-warnings 0 \"{src,mock}/**/*.{vue,ts,tsx,js,jsx}\" --fix",
    "preview": "vite preview",
    "sort-json": "npx sort-package-json \"(?!package-lock.json)*.json\" \"*(?!node_modules)/*.json\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run sort-json",
      "npm run eslint",
      "git add"
    ]
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "cspell": "^5.18.5",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.54.0",
    "stylelint": "^14.5.2",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-singlefile": "^0.6.3",
    "vue-tsc": "^0.31.4"
  }
}
